, setIsOpened] = useState(false);\r\n\r\n    const onPreviewHover = useCallback(() => {\r\n        if (openable) {\r\n            setIsLayerShown(true);\r\n        }\r\n    }, [setIsLayerShown, openable]);\r\n\r\n\r\n    const onPreviewBlur = useCallback(() => {\r\n        setIsLayerShown(false);\r\n    }, [setIsLayerShown]);\r\n\r\n    const onPreviewImageClick = useCallback(() => {\r\n        setIsOpened(true);\r\n    }, [setIsOpened]);\r\n\r\n    const onGlobalIndexLayerClick = useCallback((e?: React.MouseEvent<HTMLDivElement, MouseEvent>) => {\r\n        e?.stopPropagation();\r\n        setIsOpened(false);\r\n    }, [setIsOpened]);\r\n\r\n    return <>\r\n        <ImageContainerElement>\r\n            {isLayerShown && <>\r\n                <LayerElement onMouseOut={onPreviewBlur} onClick={onPreviewImageClick}></LayerElement>\r\n                <LayerTextElement>Open</LayerTextElement>\r\n            </>}\r\n            <PreviewImageElement onMouseOver={onPreviewHover} src={src} alt={alt} />\r\n        </ImageContainerElement>\r\n        {isOpened && <>\r\n            <ZIndexLayer onClick={onGlobalIndexLayerClick} />\r\n            <FullImageView url={src} alt={alt} additionalText={additionalText} />\r\n        </>}\r\n    </>\r\n}","D:\\words_app\\app\\client\\redux-toolkit\\src\\shared\\components\\ImageView\\elements.tsx",[],"D:\\words_app\\app\\client\\redux-toolkit\\src\\shared\\styles\\keyframes.ts",[],"D:\\words_app\\app\\client\\redux-toolkit\\src\\shared\\components\\FullImageView\\FullImageView.tsx",[],"D:\\words_app\\app\\client\\redux-toolkit\\src\\shared\\components\\FullImageView\\elements.tsx",[],"D:\\words_app\\app\\client\\redux-toolkit\\src\\components\\shared\\forms\\AddWordForm.tsx",["535","536","537","538","539","540","541","542","543","544","545"],"import React, { useCallback } from 'react';\r\nimport { Formik } from 'formik';\r\nimport { ImageView, Input, Modal, Select, TextButton } from '../../../shared';\r\nimport { AddFormElement, AddFor